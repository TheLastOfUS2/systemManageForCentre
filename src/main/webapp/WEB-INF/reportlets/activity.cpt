<Query>
<![CDATA[select
(@rowNum:=@rowNum+1) as rowNo,
a.*
from (
select 
 aa.activity_name,aa.activity_theme,aa.activity_speaker,aa.activity_time,aa.activity_content,aa.activity_remarks 
FROM am_activity aa
where 1=1 and aa.activity_del=0
${if(activityName == "",""," and aa.activity_name like '%" + activityName +"%'")} 
${if(activityTheme == "",""," and aa.activity_theme like '%" + activityTheme +"%'")} 
${if(activityTime == "",""," and aa.activity_time like '%" + activityTime +"%'")}
${if(activitySpeaker == "",""," and aa.activity_speaker like '%" + activitySpeaker +"%'")}
order by aa.activity_create_time desc 
) a  
left JOIN (SELECT @rownum:=0) co on 1=1
order by rowNo ]]></Query>
</TableData>
</TableDataMap>
